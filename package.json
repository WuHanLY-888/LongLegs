{
  "name": "vite-vue3-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite/config.dev.ts ",
    "preview": "vite preview --port=3000",
    "build": "run-p check-type build-prod",
    "build-prod": "rimraf dist && vite build --config vite/config.prod.ts",
    "check-type": "vue-tsc --noEmit",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{vue,ts,tsx,cts,mts}\"",
    "fmt": "dprint fmt"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^4.0.0",
    "vue": "^3.5.3",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "0.1.3",
    "@vueuse/core": "^11.0.3",
    "autoprefixer": "^10.4.20",
    "dprint": "^0.47.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.78.0",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.3",
    "vite-plugin-checker": "latest",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mkcert": "^1.17.6",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  }
}
